<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd" >
<sqlMap namespace="empcopy">
	<insert id="insertEmp">
		insert into empcopy (empno,ename,sal,deptno) values (8000,"gildong",3000,20)
	</insert>

	<insert id="insertEmp2" parameterClass="java.lang.String">
		insert into empcopy (empno,ename,sal,deptno) values (8002,#value#,3000,10)
	</insert>

	<insert id="insertEmp3" parameterClass="com.encore.j0716.Emp">
		insert into empcopy (empno,ename,sal,deptno) values (#empno#,#ename#,#sal#,#deptno#)
		<!--#empno#,#ename#,#sal#,#deptno# get,set 한거 넣어 준다. -->
	</insert>

	<insert id="insert_Emp_MARTIN" parameterClass="com.encore.j0716.Emp">
		insert into empcopy (ename) values (#ename#)
		<!--#empno#,#ename#,#sal#,#deptno# get,set 한거 넣어 준다. -->
	</insert>
	
	<!--8000번 사번의 사원삭제-->
	<delete id="delete">
		delete from empcopy where empno=8000
	</delete>

	<delete id="delete_Emp_MARTIN" parameterClass="String">
		delete from empcopy where ename=#ename#
	</delete>
	
</sqlMap>
